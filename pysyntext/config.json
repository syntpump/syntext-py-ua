{
	"DB": {
		"$location": "libs.db",
		"host": "atlas",
		"dbname": "syntextua"
	},
	"MorphologyRecognizer": {
		"$location": "libs.morphology",
		"collection": {
			"object": "lambda",
			"data" : "xposrules",
			"name": "collection"
		},
		"tagparser": {
			"object": "class",
			"name": "UDTParser"
		},
		"priorityList": {
			"object": "jsonfp",
			"address": "dominations.json"
		},
		"applierFunc": {
			"object": "function",
			"name": "libs.morphology,MorphologyRecognizer,selectByEnding"
		}
	},
	"ContextualProcessor": {
		"$location": "libs.ctxmorph",
		"collection": "xposrules",
		"applier": {
			"object": "function",
			"name": "libs.morphology,MorphologyRecognizer,selectByEnding"
		},
		"priority": "dominations.json",
		"tagparser": {
			"object": "class",
			"name": "MTEParser"
		},
		"rulescoll": "ctxrules"
	},
	"ConlluReader": {
		"$location": "libs.ud.conllu",
		"fp": {
			"object": "fp",
			"address": {
				"object": "sysvar",
				"name": "CONLLUPATH"
			}
		},
		"ignoreComments": false,
		"strict": false,
		"replaceMTE": true
	},
	"MTEParser": {
		"$location": "libs.ud.mte"
	},
	"UDTParser": {
		"$location": "libs.ud.udt"
	}
}
